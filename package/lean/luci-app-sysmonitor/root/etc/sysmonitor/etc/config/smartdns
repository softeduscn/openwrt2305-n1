config domain-rule

config domain-rule-list
	option enabled '1'
	option server_group 'oversea'
	option domain_list_file '/etc/smartdns/domain-set/ipv4list'
	option block_domain_type 'ipv6'
	option name 'ipv4'
	option force_aaaa_soa '1'
	option speed_check_mode 'none'
	option dualstack_ip_selection 'no'

config download-file
	option url 'https://cokebar.github.io/gfwlist2dnsmasq/gfwlist_domain.txt'
	option name 'gfwlist'
	option type 'list'

config domain-rule-list
	option enabled '1'
	option name 'gfwlist'
	option domain_list_file '/etc/smartdns/domain-set/gfwlist'
	option block_domain_type 'none'
	option server_group 'oversea'

config ip-rule

config client-rule
	option enabled '0'
	option force_https_soa '1'
	option dualstack_ip_selection '1'

